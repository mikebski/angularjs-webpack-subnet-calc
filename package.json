{
  "name": "angular-webpack-network-calculator",
  "version": "1.0.0",
  "description": "Webpack example for Angular 1.7 script",
  "scripts": {
    "build": "rimraf dist && webpack --bail --progress --profile --mode=production --env.production",
    "modules": "webpack --display-modules",
    "server": "webpack-dev-server --mode=development --env.production=false",
    "start": "npm run server",
    "test": "karma start",
    "test-watch": "karma start --auto-watch --no-single-run",
    "prd": "http-server dist"
  },
  "dependencies": {
    "angular": "^1.7.8"
  },
  "main": "main-app.js",
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "babel-loader": "^8.0.5",
    "copy-webpack-plugin": "^5.0.2",
    "css-loader": "^2.1.1",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "http-server": "^0.11.1",
    "style-loader": "^0.23.1",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.1",
    "webpack-dev-server": "^3.3.1"
  }
}
